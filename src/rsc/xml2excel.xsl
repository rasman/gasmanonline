<?xml version="1.0" encoding="utf-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:output method="xml" indent="yes"/>	<xsl:template match="/">		<results>			<xsl:apply-templates select="gasman/settings/results"/>		</results>	</xsl:template>	<xsl:template match="result">		<xsl:for-each select="agentresult">			<result>				<xsl:attribute name="time">					<xsl:value-of select="../@time"/>				</xsl:attribute>				<xsl:attribute name="agent">					<xsl:value-of select="@name"/>				</xsl:attribute>				<xsl:attribute name="uptake">					<xsl:value-of select="@uptake"/>				</xsl:attribute>				<xsl:attribute name="delivered">					<xsl:value-of select="@delivered"/>				</xsl:attribute>				<xsl:for-each select="compartments/compartment">					<xsl:attribute name="{@name}">						<xsl:value-of select="@value"/>					</xsl:attribute>				</xsl:for-each>			</result>		</xsl:for-each>	</xsl:template></xsl:stylesheet>